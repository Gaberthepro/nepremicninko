
<% @posts.each do |post| %>


<div class="card-deck">
    <div class="card">
        <a href="<%= post_path(post) %>">
            <div class="card-body">
                <h3 class="card-title"><%= post.title %></h3>
                <div class="aminities">
            </div>
                <p class="card-text"><%= post.summary%></p>
    </div>
</div>



<% end %>